#!/usr/bin/env node

var fs = require("fs");
var main = require("../");

main({
  start_path: process.argv[2],
  loadPath: function(path, callback){
    fs.readFile(path, "utf-8", callback);
  },
  global_symbols: {
    "require": true,
    "console": true
  }
}, function(err, js){
  if(err) throw err;
  console.log(js);
});
