{
  "name": "ecmaless",
  "version": "0.0.0",
  "description": "A small compile to JS language.",
  "main": "index.js",
  "bin": {
    "ecmaless": "bin/ecmaless"
  },
  "scripts": {
    "dev": "onchange '**/*.ecmaless' '**/*.js' -i -- npm run dev:test -s",
    "dev:test": "npm test -s | tap-min",
    "test": "node tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farskipper/ecmaless.git"
  },
  "keywords": [
    "language",
    "lisp",
    "scheme",
    "racket",
    "clojure",
    "freedom",
    "minimalism"
  ],
  "author": "farskipper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farskipper/ecmaless/issues"
  },
  "homepage": "https://github.com/farskipper/ecmaless#readme",
  "dependencies": {
    "concat-stream": "^1.5.1",
    "ecmaless-compiler": "^0.1.0",
    "ecmaless-parser": "^0.4.0",
    "escodegen": "^1.8.0",
    "lodash": "^4.13.1"
  },
  "devDependencies": {
    "onchange": "^2.5.0",
    "tape": "^4.6.0"
  }
}
