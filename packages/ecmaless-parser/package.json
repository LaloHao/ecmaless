{
  "name": "ecmaless-parser",
  "version": "0.0.0",
  "description": "Parser for the ecmaless language",
  "main": "index.js",
  "scripts": {
    "start": "onchange '*.js' '*.ne' -e 'grammar.js' -i -- npm test",
    "build": "nearleyc grammar.ne > grammar.js && node genSpec.js > spec.md",
    "test": "npm run build && node tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farskipper/ecmaless-parser.git"
  },
  "keywords": [
    "ecmaless",
    "parser",
    "lang"
  ],
  "author": "farskipper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farskipper/ecmaless-parser/issues"
  },
  "homepage": "https://github.com/farskipper/ecmaless-parser#readme",
  "devDependencies": {
    "json-stringify-pretty-compact": "^1.0.1",
    "lodash": "^4.13.1",
    "onchange": "^2.5.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "escape-regexp": "0.0.1",
    "estree-loc": "^1.0.2",
    "nearley": "^2.6.0",
    "tokenizer2": "^2.0.0"
  }
}
