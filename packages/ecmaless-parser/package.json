{
  "name": "ecmaless-parser",
  "version": "0.6.4",
  "description": "Parser for the ecmaless language",
  "main": "src/index.js",
  "files": [
    "src/"
  ],
  "scripts": {
    "start": "onchange 'src/' -e 'src/grammar.js' -i -- npm test",
    "build": "nearleyc src/grammar.ne > src/grammar.js && node src/genSpec.js > spec.md",
    "lint": "eslint --ignore-pattern 'src/grammar.*' src/ && echo lint-ok",
    "test": "npm run build && npm run lint -s && node src/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farskipper/ecmaless-parser.git"
  },
  "keywords": [
    "ecmaless",
    "parser",
    "lang"
  ],
  "author": "farskipper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/farskipper/ecmaless-parser/issues"
  },
  "homepage": "https://github.com/farskipper/ecmaless-parser#readme",
  "devDependencies": {
    "eslint": "^4.0.0",
    "json-stringify-pretty-compact": "^1.0.4",
    "lodash": "^4.17.4",
    "onchange": "^3.2.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "estree-loc": "^2.0.0",
    "excerpt-at-line-col": "^1.0.0",
    "nearley": "^2.10.1",
    "tokenizer2": "^2.0.0"
  }
}
